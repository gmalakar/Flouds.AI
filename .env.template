# Flouds AI Environment Configuration Template
# Copy this file to .env and modify as needed

# API Environment (Development/Production)
FLOUDS_API_ENV=Development

# Debug Mode (0/1)
FLOUDS_DEBUG_MODE=1

# Server Configuration
FLOUDS_HOST=0.0.0.0
FLOUDS_PORT=19690
FLOUDS_SERVER_TYPE=uvicorn

# ONNX Configuration (Required for Production)
FLOUDS_ONNX_ROOT=C:/path/to/your/onnx/models
FLOUDS_ONNX_CONFIG_FILE=C:/path/to/your/onnx_config.json

# Host Paths for Docker (Optional)
FLOUDS_ONNX_MODEL_PATH_AT_HOST=C:/path/to/your/onnx/models
FLOUDS_ONNX_CONFIG_FILE_AT_HOST=C:/path/to/your/onnx_config.json
FLOUDS_LOG_PATH_AT_HOST=C:/path/to/your/logs
FLOUDS_TINYDB_PATH_AT_HOST=C:/path/to/your/tinydb
# Model Session Provider
FLOUDS_MODEL_SESSION_PROVIDER=CPUExecutionProvider

# Docker Platform (Optional)
DOCKER_PLATFORM=linux/amd64